strict graph {
  bgcolor="#3f3f3f00"
  node [style=filled fillcolor="white"]
  edge [color="lightblue"]

  a [shape=none]
  b [shape=doublecircle, style=filled, fillcolor=red]
  c [shape=none]
  d
  e [shape=none]
  f [shape=doublecircle, style=filled, fillcolor=red]
  b -- a
  b -- c
  d -- b
  d -- f
  f -- e
}
